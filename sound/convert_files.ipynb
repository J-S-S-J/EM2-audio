    "import subprocess\n",
    "import os\n",
    "\n",
    "for file in os.listdir('sound_files/original_sound'):\n",
    "    if file.endswith('.mp3'):\n",
    "        subprocess.call(['ffmpeg', '-i', f'sound_files/original_sound/{file}',\n",
    "                   f'sound_files/converted_sound/{file.split(\".\")[0]}.wav'])\n",
    "\n"
   ]
  }
